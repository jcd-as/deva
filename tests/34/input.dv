# test slicing

# string slices
local a = "012345";
local s1 = a.slice( 2, 5 );
# "234"
print( s1 );
local s2 = a.slice( 1, 5, 3 );
#"14"
print( s2 );

# vector slices
local v = [];
v.append( 0 );
v.append( 1 );
v.append( 2 );
v.append( 3 );
v.append( 4 );
v.append( 5 );
local slice1 = v.slice( 2, 5 );
# 2, 3, 4
print( slice1 );
local slice2 = v.slice( 1, 5, 3 );
# 1, 4
print( slice2 );

# test maps

# create a new map
m = {};

# add some items
m[0] = "a";
m[1] = "b";
m[2] = "c";

# make a copy of m
n = copy( m );
# and change an item
n[0] = "z";

# print m
print( "\toriginal:" );
for( key, val in m )
{
	print( "key: " + str( key ) + " value: " + str( val ) );
}

print( "copy:" );
# print n (the copy)
for( key, val in n )
{
	print( "key: " + str( key ) + " value: " + str( val ) );
}

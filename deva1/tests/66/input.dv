# 'continue' (and 'break') tests
for( i in range( 0, 2 ) )
{
	print( i );
	while( true )
	{
		if( i == 0 )
			break;
		for( j in range( 0, 2 ) )
		{ 
			if( true )
			{
				break;
			}
		}
		if( i == 1 )
			break;
		if( true )
		{
			continue;
		}
	}
}


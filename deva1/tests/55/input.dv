# map initializer test cases
def Foo()
{
	return { 0 : "a" };
}
d = { Foo() : "foo" };
print( d );

d = { "foo" : 0, "bar":1 };
print( d );

class A
{
}
class B
{
}
a = new A();
b = new B();
d = { 0:"0",
	1:"1",
	2:"2",
	3:"3",
	true:"true",
	false:"false",
	null:"null",
	name(A):name(A),
	name(B):name(B),
	name(a):name(a),
	name(b):name(b)
};
print( d );

